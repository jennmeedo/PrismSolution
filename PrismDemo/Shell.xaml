<Window x:Class="PrismDemo.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:local="clr-namespace:PrismDemo"
        mc:Ignorable="d"
        Title="Shell" >
    <DockPanel LastChildFill="true">
        <ContentControl DockPanel.Dock="Top">
            <StackPanel  Orientation="Horizontal" prism:RegionManager.RegionName="ToolbarRegion"></StackPanel>
        </ContentControl>
        <ContentControl prism:RegionManager.RegionName="ContentRegion"></ContentControl>
    </DockPanel>

</Window>
